{
  "name": "simplesearchjs",
  "version": "1.0.0",
  "description": "Gmail like string search.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --outDir ./lib",
    "bench": "npm run build && node ./bench.js",
    "lint": "prettier --parser typescript --single-quote --trailing-comma es5 --tab-width 4 --write \"{src,__tests__}/**/*.ts{,x}\"",
    "precommit": "npm run lint",
    "test": "jest --coverage --verbose"
  },
  "dependencies": {
    "tslib": "^1.5.0"
  },
    "devDependencies": {
        "node": ">=6.0.0",
    "@types/jest": "^16.0.1",
    "@types/node": "^7.0.13",
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "benchmark": "^2.1.4",
    "coveralls": "^2.13.0",
    "husky": "^0.13.3",
    "jest": "^17.0.3",
    "prettier": "prettier/prettier#4b3835925e87706c66f88efc4d94dadd3ce0db1c",
    "ts-loader": "^1.3.3",
    "typescript": "^2.0.8",
    "typescript-eslint-parser": "^2.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
