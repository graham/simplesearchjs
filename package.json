{
  "name": "simplesearchjs",
  "version": "2.1.0",
  "description": "Gmail like string search.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --outDir ./lib",
    "bench": "npm run build && node ./bench.js",
    "lint": "prettier --parser typescript --single-quote --trailing-comma es5 --tab-width 4 --write \"{src,__tests__}/**/*.ts{,x}\"",
    "test": "jest --coverage --verbose"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.26",
    "babel-core": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.18.0",
    "benchmark": "^2.1.4",
    "coveralls": "^2.13.3",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "prettier": "github:prettier/prettier#4b3835925e87706c66f88efc4d94dadd3ce0db1c",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "ts-loader": "^2.3.4",
    "tslib": "^1.9.0",
    "typescript": "^2.7.2",
    "typescript-eslint-parser": "^7.0.0",
    "webpack": "^3.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
